    {
      "cell_type": "code",
      "source": [
        "def remove_stopwords (text):\n",
        "  new_text = []\n",
        "  for word in text.split():\n",
        "    if word.lower() in stopwords.words('english'):\n",
        "      new_text.append('')\n",
        "    else:\n",
        "      new_text.append(word)\n",
        "  return \" \".join(new_text)"
      ],
      "metadata": {
        "id": "sl-btD_5Tx5W"
      },
      "execution_count": 39,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "text = remove_stopwords (text)\n",
        "text"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 36
        },
        "id": "98k8oXymUH2V",
        "outputId": "59ad63b6-5fa2-400c-864d-33a2c448c175"
      },
      "execution_count": 40,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
