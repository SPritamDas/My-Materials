        "### For Strings"
      ],
      "metadata": {
        "id": "9te_9bMHJa1v"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def chat_conversion(text):\n",
        "    new_text = []\n",
        "    for word in text.split():\n",
        "        if word.lower() in chat_words_dict:\n",
        "            new_text.append(chat_words_dict[word.lower()])\n",
        "        else:\n",
        "            new_text.append(word)\n",
        "    return \" \".join(new_text)"
      ],
      "metadata": {
        "id": "I6XpYIN5Jdf5"
      },
      "execution_count": 32,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "text = chat_conversion(text)\n",
        "text"
      ],
      "metadata": {
